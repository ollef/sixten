
the : (A : Type) -> A -> A
the _ x = x

test b = case b of
  True -> the (forall a. a -> a) \x. x
  False -> \x. x

test2 : Bool -> forall a. a -> a
test2 b = case b of
  True -> the (forall a. a -> a) \x. x
  False -> the (forall a. a -> a) \x. x

